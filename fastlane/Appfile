# Appfile for ONDA iOS App

# Your Apple ID (email)
# This will be set via environment variable: FASTLANE_USER
apple_id(ENV["FASTLANE_USER"] || "your-apple-id@example.com")

# App Identifier (Bundle ID)
app_identifier("com.onda-life.ios")

# iTunes Connect Team ID
# This will be set via environment variable: FASTLANE_ITC_TEAM_ID
itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"])

# Apple Developer Team ID
# This will be set via environment variable: FASTLANE_TEAM_ID
team_id(ENV["FASTLANE_TEAM_ID"])
